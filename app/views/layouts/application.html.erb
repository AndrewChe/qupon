<!DOCTYPE html>
<html>
<head>
  <title>Qap</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= link_to "Home", '/'  %>
<% if signed_in? %>
    <%= link_to 'Sign out', sign_out_path, :method => :delete %>
    <%= "Hi, #{current_user.name}" %>
<% else %>
    <%= link_to 'Sign in', sign_in_path  %>
<% end %>
<%= yield %>

</body>
</html>
